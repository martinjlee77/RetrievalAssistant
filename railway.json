{
  "services": {
    "website": {
      "source": {
        "repo": ".",
        "rootDirectory": "."
      },
      "build": {
        "builder": "NIXPACKS"
      },
      "deploy": {
        "startCommand": "python backend_api.py --port $PORT",
        "healthcheckPath": "/api/health"
      }
    },
    "analysis": {
      "source": {
        "repo": ".",
        "rootDirectory": "."
      },
      "build": {
        "builder": "NIXPACKS"
      },
      "deploy": {
        "startCommand": "streamlit run home.py --server.port $PORT --server.address 0.0.0.0 --server.headless true"
      }
    }
  }
}